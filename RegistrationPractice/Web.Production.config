<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <!--
    In the example below, the "SetAttributes" transform will change the value of 
    "connectionString" to use "ReleaseSQLServer" only when the "Match" locator 
    finds an attribute "name" that has a value of "MyDB".
     -->
  <connectionStrings>
    <add name="DefaultConnection"
      connectionString="Data Source=SQL5040.site4now.net;Initial Catalog=DB_A3E1A3_arod1979;User Id=DB_A3E1A3_arod1979_admin;Password=A1l0l5a4;" providerName="System.Data.SqlClient"
      xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    <!--<add name="EmailConnection"
      connectionString="Data Source=SQL5039.site4now.net;Initial Catalog=DB_A3E1A3_email;User Id=DB_A3E1A3_email_admin;Password=A1l0l5a4;" providerName="System.Data.SqlClient"
      xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>-->
  </connectionStrings>
  <appSettings>
    <add key="collectstats" value="true" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
  </appSettings>



  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <!--
      In the example below, the "Replace" transform will replace the entire 
      <customErrors> section of your web.config file.
      Note that because there is only one customErrors section under the 
      <system.web> node, there is no need to use the "xdt:Locator" attribute.
      -->
    <customErrors defaultRedirect="GenericError.htm"
      mode="Off" xdt:Transform="Replace">
      <error statusCode="500" redirect="InternalError.htm"/>
    </customErrors>

  </system.web>

  <appSettings>
    <add key="servername" value="http://awolr.com" xdt:Transform="SetAttributes"
          xdt:Locator="Match(key)"/>

    <add key="emailpassword" value="passWord321$"  xdt:Transform="SetAttributes"
          xdt:Locator="Match(key)"/>
    <add key="email" value="admin@awolr.com"  xdt:Transform="SetAttributes"
          xdt:Locator="Match(key)"/>

  </appSettings>
</configuration>